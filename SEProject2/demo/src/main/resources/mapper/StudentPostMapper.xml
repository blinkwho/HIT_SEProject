<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sepj.mapper.StudentPostMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.sepj.entity.StudentPost">
        <id column="id" property="id" />
        <result column="subject" property="subject" />
        <result column="telephone_number" property="telephoneNumber" />
        <result column="student_id" property="studentId" />
        <result column="price" property="price" />
        <result column="start_date" property="startDate" />
        <result column="end_date" property="endDate" />
        <result column="remark" property="remark" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, subject, telephone_number, student_id, price, start_date, end_date, remark
    </sql>

</mapper>
